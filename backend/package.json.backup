{
  "name": "datahub-backend",
  "version": "1.0.0",
  "description": "DataHub Backend API",
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "typeorm": "npx typeorm",
    "lint-staged": "lint-staged",
    "migration:generate": "npx typeorm migration:generate -d ormconfig.js -n",
    "migration:run": "npx typeorm migration:run -d ormconfig.js",
    "migration:revert": "npx typeorm migration:revert -d ormconfig.js"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/jwt": "^10.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/typeorm": "^11.0.0",
    "@types/argon2": "^0.14.1",
    "@types/bcrypt": "^6.0.0",
    "@types/passport-jwt": "^4.0.1",
    "argon2": "^0.44.0",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "helmet": "^8.1.0",
    "nestjs-pino": "^4.4.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
    "pino-http": "^10.5.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.0",
    "undefined": "^0.1.0"
  },
  "devDependencies": {
    "@nestjs/schematics": "^9.0.0",
    "@types/compression": "^1.8.1",
    "@types/express": "^4.17.23",
    "@types/helmet": "^0.0.48",
    "@types/node": "^18.19.124",
    "lint-staged": "^15.5.2",
    "pino-pretty": "^13.1.1",
    "typeorm": "^0.3.26",
    "typescript": "^4.9.5"
  }
}
